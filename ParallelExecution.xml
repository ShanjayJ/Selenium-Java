<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite guice-stage="DEVELOPMENT" name="Vmetry TestNG" parallel="tests" thread-count="2">
	<test verbose="2" name="Regression Execution">
		<groups>
			<run>
				<include name="all"></include>
				<include name="regression"></include>				
			</run>
		</groups>
		<classes>
			<class name="com.vmetry.testng.Grouping" />
		</classes>
	</test> <!-- Default test -->
	
	<test verbose="2" name="Sanity Execution">
		<groups>
			<run>
				<include name="all"></include>
				<include name="sanity"></include>				
			</run>
		</groups>
		<classes>
			<class name="com.vmetry.testng.Grouping" />
		</classes>
	</test> <!-- Default test -->
</suite> <!-- Default suite -->
           